# Environment variables for KILOS API

# Server
PORT=3001
NODE_ENV=development

# ML Service
ML_SERVICE_URL=http://localhost:8000

# Database (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=kilos_db
DB_USER=postgres
DB_PASSWORD=your_password

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_key_change_in_production

# Meralco API (if integrating with existing systems)
MERALCO_API_URL=https://api.meralco.com.ph
MERALCO_API_KEY=your_api_key
